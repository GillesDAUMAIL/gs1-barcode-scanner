<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="cache" />
        <exclude domain="no_backup" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow data transfer between devices -->
        <include domain="file" />
    </device-transfer>
</data-extraction-rules>